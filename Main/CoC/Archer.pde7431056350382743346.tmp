public class Archer extends Monster{
    
  PImage icon;
  
  
   public Archer(){
     super();
     _hp = 100;
     _maxHP = 100;
     _mp = 50;
     _trainingTime = 25;
     _basicSpeed = 5;
     _attackRange = 50;
     _dmgPerAttack=7;
     _level = 1;
     _houseSpace = 1;
      _name = "Archer";
      icon = loadImage("barb.png");
       }
   
   public Archer(int x, int y){
     this();
     _xcor = x;
     _ycor = y;
   }
   
   void draw(){
      super.draw();
    //image(icon, _xcor + icon.width/2, _ycor + icon.height/2, 50, 50);
    imageMode(CENTER);
    image(icon, _xcor, _ycor, 50, 50);
    imageMode(CORNER);
    fill(0); 
   text(_hp + " ", _xcor+70,  _ycor+40 ); 
    
    }


}